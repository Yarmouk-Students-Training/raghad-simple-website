<html lang="en">
    <%- include('./partials/head.ejs') %>
    <body>

        <% const name = 'Raghad' %> 
        <%- include('./partials/nav.ejs') %>  
       
        <div class="blogs contaent">
            <h2>ALL Blogs</h2>

            <% if (blogs.length > 0) { %>
            <% blogs.forEach (blog =>{ %>
            <a class="single" href="/blogs/<%= blog._id %>">
            <h3 class="title"><%= blog.title %></h3>
            <p class="snippet"><%= blog.snippet%></p>
            </a> 
            <% })%>
            <%} %>


        </div>
        <%- include('./partials/footer.ejs') %>
    </body>
</html>